# http://editorconfig.org

root = true

# Default
[*]
indent_style = tab
tab_width = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false

[*]
indent_style = space
tab_width = 4

# Markdown
[*.md]
trim_trailing_whitespace = false

# JSON
[*.json]
indent_style = space
indent_size = 2
insert_final_newline = true